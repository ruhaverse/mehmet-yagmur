# ShareUpTime Mobile App - Production Environment

# Production Environment
REACT_APP_ENV=production
REACT_APP_API_BASE_URL=https://api.shareuptime.com
REACT_APP_WS_BASE_URL=wss://api.shareuptime.com/ws
REACT_APP_ENABLE_DEBUGGING=false
REACT_APP_ENABLE_PERFORMANCE_MONITORING=true

# API Configuration
REACT_APP_API_TIMEOUT=20000
REACT_APP_API_RETRY_ATTEMPTS=2
REACT_APP_API_CACHE_DURATION=600000

# WebSocket Configuration
REACT_APP_WS_RECONNECT_ATTEMPTS=3
REACT_APP_WS_RECONNECT_INTERVAL=3000
REACT_APP_WS_HEARTBEAT_INTERVAL=45000

# Performance Configuration
REACT_APP_IMAGE_CACHE_SIZE=200
REACT_APP_MEMORY_WARNING_THRESHOLD=75
REACT_APP_BUNDLE_ANALYZER=false

# Feature Flags
REACT_APP_ENABLE_REAL_TIME=true
REACT_APP_ENABLE_OFFLINE_MODE=true
REACT_APP_ENABLE_SMART_FEED=true
REACT_APP_ENABLE_PERFORMANCE_OPTIMIZATION=true
REACT_APP_ENABLE_ADVANCED_CACHING=true

# Analytics Configuration
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ANALYTICS_ENDPOINT=https://analytics.shareuptime.com

# Security Configuration
REACT_APP_ENABLE_SSL_PINNING=true
REACT_APP_ENABLE_ROOT_DETECTION=true